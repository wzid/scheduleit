  "type": "module",
  "dependencies": {
    "@libsql/client": "^0.15.1",
    "argon2": "^0.41.1",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.41.0",
    "lucide-svelte": "^0.485.0",
    "nanoid": "^5.0.5",
    "tailwind-merge": "^3.0.2"
